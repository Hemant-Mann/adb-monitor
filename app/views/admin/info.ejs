<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row col-lg-12">
            <h1 class="page-header text-center"><%= model %></h1>
            <div class="panel panel-default">
                <table class="table table-striped text-center table-hover">
                    <tbody>
                        <% for (prop in item) { %>
                        <tr>
                            <th><%= prop %></th>
                            <th><%= item[prop] %></th>
                        </tr>
                        <% } %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
