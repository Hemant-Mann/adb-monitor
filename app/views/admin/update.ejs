<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row col-lg-12">
            <h1 class="page-header text-center"><%= model %></h1>
            <% if (success) { %>
            <div class="alert alert-success alert-dismissible fade in" role="alert">
                <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">Ã—</span></button>
                <%= model %> <strong>saved Successfully!!</strong>
            </div>
            <% } %>
            <div class="col-md-offset-1 col-lg-8">
                <form class="form-horizontal" method="post" action="">
                    <% for (prop in item) { %>
                    <div class="form-group">
                        <label class="col-sm-4 control-label"><%= prop %></label>
                        <div class="col-sm-8">
                            <input type="text" name="<%= prop %>" class="form-control" value='<%= item[prop] %>' <% if (disabled.indexOf(prop) !== -1){ %> disabled=""<% } %>>
                        </div>
                    </div>
                    <% } %>
                    <input type="hidden" name="action" value="update">
                    <button type="submit" class="btn btn-success pull-right">Save <i class="fa fa-check-circle fa-fw"></i></button>
                </form>
            </div>
        </div>
    </div>
</div>
