<div class="wrapper-page">

    <div class="text-center">
        <a href="/index.html" class="logo logo-lg"><i class="md md-equalizer"></i> <span><%= seo.platform %></span> </a>
    </div>
    <% if (message) { %>
	<div class="alert alert-info alert-dismissible fade in" role="alert">
	    <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">Ã—</span></button>
	    <%= message %>
	</div>
	<% } %>
    <form class="form-horizontal m-t-20" method="post">

        <div class="form-group">
            <div class="col-xs-12">
                <input class="form-control" type="email" name="email" required="" placeholder="Registered Email">
                <i class="md md-account-circle form-control-feedback l-h-34"></i>
            </div>
        </div>

        <div class="form-group text-right m-t-20">
            <div class="col-xs-12">
                <button class="btn btn-primary btn-custom w-md waves-effect waves-light" type="submit">Submit Request
                </button>
            </div>
        </div>

        <div class="form-group m-t-30">
            <div class="col-sm-5 text-right">
                <a href="/" class="text-muted">Go Back</a>
            </div>
        </div>
    </form>
</div>
